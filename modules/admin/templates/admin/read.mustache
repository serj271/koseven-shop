<div class="page-header">
	<h1>{{headline}}</h1>
</div>

<div class="well">
	{{#buttons}}
	<a class="btn {{class}}" href="{{url}}">{{text}}</a>
	{{/buttons}}
</div>

{{#values}}
<div class="row">
	<div class="span4">
		<strong>{{label}}</strong>
	</div>
	<div class="span12">{{value}}</div>
</div>
{{/values}}